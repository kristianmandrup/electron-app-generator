{
  "name": "electron-app-generator",
  "version": "0.0.1",
  "description": "Electron app generator",
  "scripts": {
    "build": "node tasks/release.js",
    "build:clean": "cross-env PLATFORM_TARGET=clean node tasks/release.js",
    "build:darwin": "cross-env PLATFORM_TARGET=darwin node tasks/release.js",
    "build:linux": "cross-env PLATFORM_TARGET=linux node tasks/release.js",
    "build:mas": "cross-env PLATFORM_TARGET=mas node tasks/release.js",
    "build:win32": "cross-env PLATFORM_TARGET=win32 node tasks/release.js",
    "dev": "node tasks/runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix app test",
    "pack": "cross-env NODE_ENV=production webpack -p --progress --colors",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "ava": "ava",
    "vue:route": "node tasks/vue/route.js",
    "vuex:module": "node tasks/vuex/module.js",
    "postinstall": "npm run lint:fix && cd app && npm install"
  },
  "ava": {
    "files": [
      "test/ava/**/*.spec.js"
    ],
    "source": [
      "app/src/**/*.{js,jsx}"
    ],
    "concurrency": 5,
    "failFast": true,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Greg Holguin <simulatedgreg@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^1.3.1",
    "electron-devtools-installer": "^2.1.0",
    "electron-packager": "^8.0.0",
    "electron-rebuild": "^1.1.3",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.0",
    "html-webpack-plugin": "^2.16.1",
    "inject-loader": "^3.0.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.0.2",
    "require-dir": "^0.3.0",
    "spectron": "^3.4.0",
    "style-loader": "^0.16.1",
    "tree-kill": "^1.1.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.0.11",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^11.3.4",
    "vue-style-loader": "^2.0.5",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "ava": "^0.18.2",
    "lodash.debounce": "^4.0.8",
    "rxjs": "^5.0.0-rc.2",
    "vue": "^2.0.5",
    "vue-material": "marcosmoura/vue-material#master",
    "vue-rx": "^3.0.0",
    "vue2-dragula": "^2.1.1",
    "vuex-connect": "^1.1.1"
  }
}
